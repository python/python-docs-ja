#: ../../library/logging.rst:775
msgid "Name of function containing the logging call."
msgstr "ロギングの呼び出しを含む関数の名前。"

#: ../../library/logging.rst:777
msgid "levelname"
msgstr "levelname"

#: ../../library/logging.rst:777
msgid "``%(levelname)s``"
msgstr "``%(levelname)s``"

