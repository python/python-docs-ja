msgstr "40"

#: ../../library/logging.rst:355
msgid "``WARNING``"
msgstr "``WARNING``"

#: ../../library/logging.rst:355
msgid "30"
msgstr "30"

#: ../../library/logging.rst:357
msgid "``INFO``"
msgstr "``INFO``"

#: ../../library/logging.rst:357
msgid "20"
msgstr "20"

#: ../../library/logging.rst:359
msgid "``DEBUG``"
msgstr "``DEBUG``"

#: ../../library/logging.rst:359
msgid "10"
msgstr "10"

