msgstr "10 進浮動小数点数。"

#: ../../library/stdtypes.rst:1524
msgid "``'F'``"
msgstr "``'F'``"

#: ../../library/stdtypes.rst:1526
msgid "``'g'``"
msgstr "``'g'``"

