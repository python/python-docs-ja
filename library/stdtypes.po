msgstr "``'i'``"

#: ../../library/stdtypes.rst:1510
msgid "``'o'``"
msgstr "``'o'``"

#: ../../library/stdtypes.rst:1510
msgid "Signed octal value."
msgstr "符号付き 8 進数。"

#: ../../library/stdtypes.rst:1512
msgid "``'u'``"
msgstr "``'u'``"

#: ../../library/stdtypes.rst:1512
